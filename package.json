{
  "name": "polythene-mithril-setup",
  "description": "Polythene for Mithril setup example",
  "version": "1.2.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ArthurClemens/polythene-mithril-setup.git"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:watch dev:serve",
    "dev:serve": "webpack-dev-server --config scripts/webpack.config.dev.js --port 3000",
    "dev:watch": "webpack --watch --config scripts/webpack.config.dev.js",
    "build": "webpack --config scripts/webpack.config.prod.js"
  },
  "dependencies": {
    "mithril": "1.1.6",
    "polythene-css": "1.2.0",
    "polythene-mithril-dialog": "1.2.0",
    "polythene-mithril-raised-button": "1.2.0"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-loader": "7.1.4",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "compression-webpack-plugin": "1.1.11",
    "css-loader": "0.28.11",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "npm-run-all": "4.1.2",
    "style-loader": "0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "4.6.0",
    "webpack-cli": "2.1.2",
    "webpack-dev-server": "3.1.3"
  }
}
